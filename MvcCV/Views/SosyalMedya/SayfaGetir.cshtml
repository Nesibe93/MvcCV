@model MvcCV.Models.Entity.TblSosyalMedya

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Sosyal Medya Hesap Düzenleme sayfası</h4>
<br />
<form method="post">
    @Html.Label("Sosyal Medya Ado")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("İcon")
    @Html.TextBoxFor(x => x.İcon, new { @class = "form-control" })
    <button class="btn btn-success">Güncelle</button>
</form>

